{
  "name": "forgesteel-server",
  "version": "1.0.0",
  "description": "Room server for multiplayer Forgesteel",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "dev": "vite-node --watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "better-sqlite3": "^11.6.0",
    "cors": "^2.8.5",
    "express": "^4.21.1",
    "jsonwebtoken": "^9.0.3",
    "uuid": "^11.0.3",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.11",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^22.10.1",
    "@types/uuid": "^10.0.0",
    "@types/ws": "^8.5.13",
    "@vitest/coverage-v8": "^4.0.15",
    "@types/supertest": "^6.0.2",
    "supertest": "^7.0.0",
    "typescript": "^5.7.2",
    "vite": "^6.0.3",
    "vite-node": "^2.1.8",
    "vitest": "^4.0.15"
  }
}
